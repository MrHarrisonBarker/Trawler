{
  "name": "Trawler",
  "description": "Search reddit for link appearances",
  "version": "0.1",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["main.js"]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/roddit16.png",
      "32": "images/roddit32.png",
      "48": "images/roddit48.png",
      "128": "images/roddit128.png"
    }
  },
  "icons": {
    "16": "images/roddit16.png",
    "32": "images/roddit32.png",
    "48": "images/roddit48.png",
    "128": "images/roddit128.png"
  },
  "author": "Harrison Barker"
}